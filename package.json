{
  "name": "evan-lang",
  "version": "0.0.1",
  "description": "a vaguely lispy language in the context of js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dslmeinte/evan-lang.git"
  },
  "scripts": {
    "test": "npm run build && tape lib/test/*.js | tap-diff",
    "test-watch": "npm run build && concurrently --raw \"npm run watch\" \"tape-watch lib/test/*.js -p tap-diff\" --kill-others",
    "clean": "rm -fr lib",
    "build": "npm run clean && tsc",
    "prepublish": "npm run test && npm run build",
    "watch": "npm run clean && tsc --watch"
  },
  "contributors": [
    "Meinte Boersma <meinte.boersma@gmail.com> (http://www.dslconsultancy.com/)",
    "Onur Gunduz <ogunduz@gmail.com> (http://onurgunduz.com/)"
  ],
  "bin": {
    "evan": "lib/bin/cmd.js"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "license": "MIT",
  "dependencies": {
    "indent-string": "^3.0.0",
    "is-integer": "^1.0.6",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^6.0.55",
    "@types/tape": "^4.2.28",
    "concurrently": "^3.1.0",
    "glob": "^7.1.1",
    "ndjson": "^1.5.0",
    "split2": "^2.1.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4",
    "tslint": "^4.2.0",
    "typescript": "^2.1.4"
  }
}
